
@model List<Test_DotNetMVC.Controllers.NewsController.Article>
@{
    ViewData["Title"] = "Index";
}

<h1>List</h1>
<table class="table">
    <tr>
        <th>Id</th>
        <th>Title</th>
        <th>Content</th>
        <th>Author</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
            <td>@item.Content</td>
            <td>@item.Author</td>
        </tr>
    }
</table>
